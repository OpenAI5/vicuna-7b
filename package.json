{
  "name": "vicuna-7b",
  "description": "Programmatic access to the Vicuna 7B LLM model",
  "version": "0.1.2",
  "files": [
    "dist",
    "lib"
  ],
  "main": "./dist/Vicuna7B.umd.js",
  "types": "./dist/Vicuna7B.d.ts",
  "module": "./dist/Vicuna7B.es.js",
  "exports": {
    ".": {
      "import": "./dist/Vicuna7B.es.js",
      "require": "./dist/Vicuna7B.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "rimraf ./dist && vite build",
    "build:dev": "rimraf ./dist && vite build --config vite.config.dev.ts",
    "serve:dev": "pnpx http-server dist"
  },
  "keywords": [
    "vicuna",
    "Vicuna 7B",
    "llm",
    "chat",
    "language model",
    "nlp",
    "natural language processing",
    "AI",
    "artificial intelligence",
    "machine learning",
    "deep learning",
    "browser-based",
    "chatbot",
    "conversational AI",
    "browser-based language model",
    "natural language understanding",
    "text generation",
    "browser-based NLP",
    "text processing"
  ],
  "author": "Kevin Scott",
  "license": "Apache",
  "repository": {
    "type": "git",
    "url": "https://github.com/thekevinscott/vicuna-7b.git"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.2",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-mkcert": "^1.14.0"
  }
}
