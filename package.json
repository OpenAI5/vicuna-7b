{
  "name": "vicuna-7b",
  "description": "Programmatic access to the Vicuna 7B LLM model",
  "version": "0.0.5",
  "files": [
    "dist",
    "public"
  ],
  "main": "./dist/Vicuna7B.umd.js",
  "types": "./dist/Vicuna7B.d.ts",
  "module": "./dist/Vicuna7B.es.js",
  "exports": {
    ".": {
      "import": "./dist/Vicuna7B.es.js",
      "require": "./dist/Vicuna7B.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "rimraf ./dist && vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "vicuna",
    "Vicuna 7B",
    "llm",
    "chat",
    "Large Language Model"
  ],
  "author": "Kevin Scott",
  "license": "Apache",
  "devDependencies": {
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.2",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-mkcert": "^1.14.0"
  }
}
